<p id="notice"><%= notice %></p>

<h1>User Work Experience Roles</h1>

<table>
  <thead>
    <tr>
      <th>Role</th>
      <th>Date from</th>
      <th>Date to</th>
      <th>Project name</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_work_experience_roles.each do |user_work_experience_role| %>
      <tr>
        <td><%= user_work_experience_role.role %></td>
        <td><%= user_work_experience_role.date_from %></td>
        <td><%= user_work_experience_role.date_to %></td>
        <td><%= user_work_experience_role.project_name %></td>
        <td><%= user_work_experience_role.notes %></td>
        <td><%= link_to 'Show', user_work_experience_role %></td>
        <td><%= link_to 'Edit', edit_user_work_experience_role_path(user_work_experience_role) %></td>
        <td><%= link_to 'Destroy', user_work_experience_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Work Experience Role', new_user_work_experience_role_path %>
