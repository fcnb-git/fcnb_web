<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :family_name %><br />
    <%= f.text_field :family_name, autofocus: true%>
  </div>

  <div class="field">
    <%= f.label :given_name %><br />
    <%= f.text_field :given_name, autofocus: true%>
  </div>

  <div class="field">
    <%= f.label :middle_name %><br />
    <%= f.text_field :middle_name, autofocus: true%>
  </div>

  <div class="field">
    <%= f.label :port_of_entry %><br />
    <%= f.text_field :port_of_entry, autofocus: true%>
  </div>

  <div class="field">
    <%= f.label :pr_date %><br />
    <%= f.date_field :pr_date, autofocus: true%>
  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address, autofocus: true%>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city, autofocus: true%>
  </div>
  <div class="field">
    <%= f.label :province %><br />
    <%= f.text_field :province, autofocus: true%>
  </div>
  <div class="field">
    <%= f.label :country %><br />
    <%= f.text_field :country, autofocus: true%>
  </div>
  <div class="field">
    <%= f.label :mobile_phone %><br />
    <%= f.text_field :mobile_phone, autofocus: true%>
  </div>
  <div class="field">
    <%= f.label :home_phone %><br />
    <%= f.text_field :home_phone, autofocus: true%>
  </div>


  




  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
