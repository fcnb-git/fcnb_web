<%content_for :content_page do%>
<p id="notice"><%= notice %></p>

<h1>User Educations</h1>

<table>
  <thead>
    <tr>
      <th>Education type</th>
      <th>Institution</th>
      <th>Degree</th>
      <th>Program</th>
      <th>Specialization</th>
      <th>Date started</th>
      <th>Date completed</th>
      <th>Remarks</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_educations.each do |user_education| %>
      <tr>
        <td><%= user_education.education_type %></td>
        <td><%= user_education.institution %></td>
        <td><%= user_education.degree %></td>
        <td><%= user_education.program %></td>
        <td><%= user_education.specialization %></td>
        <td><%= user_education.date_started %></td>
        <td><%= user_education.date_completed %></td>
        <td><%= user_education.remarks %></td>
        <td><%= link_to 'Show', user_education %></td>
        <td><%= link_to 'Edit', edit_user_education_path(user_education) %></td>
        <td><%= link_to 'Destroy', user_education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Education', new_user_education_path %>


<%end%>
